{
  "expo": {
    "name": "Biu Assist",
    "slug": "biuApp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android":{
      "androidNavigationBar": {
       "visible": true,
       "backgroundColor":"red"
     }
    },
    "description": "",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg",
        "mp3"
      ]
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "Biu Assist",
  "name": "BiuAssist"
}